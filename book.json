{
    "title": "I, marslo",
    "author" : "marslo",
    "description" : "my gitbook",
    "language" : "en",
    "plugins": [
        "-lunr",
        "-search",
        "search-pro",
        "back-to-top-button",
        "expandable-chapters",
        "chapter-fold",
        "splitter",
        "accordion",
        "code",
        "pageview-count",
        "page-toc-button"
    ],
    "pluginsConfig": {
      "anchor-navigation-ex": {
        "isShowTocTitleIcon": true
      },
      "page-toc-button": {
          "maxTocDepth": 2,
          "minTocSize": 2
      }
    },
    "links" : {
        "sidebar" : {
            "I, Marslo" : "https://marslo.github.io/"
        }
    },
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    }
}
